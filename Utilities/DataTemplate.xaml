<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ViewModel="clr-namespace:Project_BlueLock.ViewModels"
                    xmlns:view="clr-namespace:Project_BlueLock.Views">
    
    <DataTemplate DataType="{x:Type ViewModel:RegistrationViewModel}">
        <view:RegistrationView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type ViewModel:LoginViewModel}">
        <view:LoginView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type ViewModel:CreateProfileViewModel}">
        <view:CreateProfileView/>
    </DataTemplate>
</ResourceDictionary>